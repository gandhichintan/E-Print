{
  "productName": "ePrinter",
  "asar": false,
  "artifactName": "${productName}-${version}.${ext}",
  "directories": {
    "app": "dist",
    "output": "app-builds"
  },
  "win": {
    "icon": "dist/favicon"
  },
  "mac": {
    "icon": "dist/favicon",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "AppImage"
    ]
  }
}
